@using EntityLayer.Concrete
@model List<Admin>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>KULLANICI MAİL</th>
        <th>YETKİ</th>
        <th>STATUS</th>
        <th>PASİF YAP</th>
        <th>YETKİSİNİ DEĞİŞTİR</th>
       
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AdminID</th>
            <td>@item.AdminUsername</td>
            <td>@item.AdminRole</td>
            <td>Aktif</td>
            <td><a href="/AdminCategory//@item.AdminID" class="btn btn-danger">Pasif Yap</a></td>
            <td><a href="/AdminCategory//@item.AdminID" class="btn btn-warning">Yetkisini Değiştir</a></td>
           
        </tr>
    }
</table>
<a href="/Authorization/AddAdmin/" class="btn btn-dark">Yeni Admin Ekle</a>
<script src="~/AdminLTE-3.0.4/plugins/jquery/jquery.min.js"></script>

